package java181010;

public class Ex02 {

	public static void main(String[] args) {
		// 2차원 배열 복습
		int[][] array =  new int[3][3];
		int num = 0;

		for (int i : array) {
			for (int j : array[i]) {
				num += 10;
				array[i][j] = num;
				System.out.print(array[i][j]);
			}
			System.out.println();
		}
	}

}
