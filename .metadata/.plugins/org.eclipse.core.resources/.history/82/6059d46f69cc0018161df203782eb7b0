package java181010;

public class SelectionSort {

	public static void main(String[] args) {
		// selection sort
		// 가장 큰 원소 또는 작은 원소를 찾아 주어진 위치(처음~끝)를 바꿈
		int temp, temp2;
		int[] array = { 98, 7, 70, 13, 24 };
		// 
		
		for (int i = 0; i < array.length; i++) {
			temp = array[i];
			for (int j = 0; j < array.length; j++) {
				if (temp < array[j]) {
					
				}
			}
			
		}
		
	}

}
