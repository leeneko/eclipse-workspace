package java181010;

public class Star {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		int num = 5;
		for (int i = 0; i < num; i++) {
			System.out.println("i/true");
			for (int j = 4; j > i; j--) { // 4 > 0 true 3 > 0 true
				System.out.print(" "); 
			}
			for (int k = 1; k > i; k++) { // 1 > 0 true
				System.out.print("*");
			}
			System.out.println();
		}
	}

}
