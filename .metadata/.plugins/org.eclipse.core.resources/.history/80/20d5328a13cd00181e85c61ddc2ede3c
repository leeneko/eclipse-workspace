package java181010;

public class SelectionSort {

	public static void main(String[] args) {
		// selection sort
		// 가장 큰 원소 또는 작은 원소를 찾아 주어진 위치(처음~끝)를 바꿈
		int temp, min, v = 0;
		int[] array = { 98, 7, 70, 13, 24 };
		// 98을 기준으로 기준과 7 비교
		// 7이 기준이 되어 기준과 70 비교
		// 기준과 13 비교
		// 기준과 24 비교
		// 기준을 처음과 치환
		// 7, 98, 70, 13, 24
		
		// 98을 기준으로 70과 비교
		// 70이 기준이 되어 기준과 13 비교
		// 13이 기준이 되어 기준과 24 비교
		// 기준을 두번째와 치환
		// 7, 13, 98, 70, 24
		
		// 98을 기준으로 70과 비교
		// 70이 기준이 되어 기준과 24비교
		// 24가 기준이 되어 세번째와 치환
		// 7, 13, 24, 98, 70
		
		for (int i = 0; i < array.length; i++) {
			
		}
	}
}
