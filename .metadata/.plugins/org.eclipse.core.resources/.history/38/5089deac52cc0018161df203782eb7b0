package java181010;

public class Ex02 {

	public static void main(String[] args) {
		// 2차원 배열 복습
		int[][] array =  new int[3][3];
		int num = 0;

		for (int[] i : array) {
			for (int j : i) {
				num += 10;
				i[j] = num;
				System.out.print(j + " ");
			}
			System.out.println();
		}
		
		for(int i = 0; i < array.length; i++) {
			for(int j = 0; j < array[i].length; j++) {
				System.out.print(array[i][j] + " ");
			}
			System.out.println();
		}
	}

}
